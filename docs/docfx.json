{
	"build": {
		"content": [
			{
				"files": [
					"articles/**.md",
					"articles/toc.yml",
					"contributing/**.md",
					"contributing/toc.yml",
					"toc.yml",
					"*.md"
				]
			},
		  {
			"files": [
			  "models/**.md",
			  "models/**/**.md",
			  "models/toc.yml",
			  "toc.yml",
			  "*.md"
			]
		  },
		  {
			"files": [
			  "scenario-samples/**.md",
			  "scenario-samples/**/**.md",
			  "scenario-samples/toc.yml",
			  "toc.yml",
			  "*.md"
			]
		  },
		  {
			"files": [
			  "site-templates/**.md",
			  "site-templates/**/**.md",
			  "site-templates/toc.yml",
			  "toc.yml",
			  "*.md"
			]
		  },
		  {
			"files": [
			  "scripts/**.md",
			  "scripts/**/**/**.md",
			  "scripts/toc.yml",
			  "toc.yml",
			  "*.md"
			]
		  }
		],
		"resource": [
			{
				"files": [
					"images/**",
					"models/**/**/*.png",
					"scenario-samples/**/**/*.png",
					"site-templates/**/**/*.png",
					"scripts/**/**/*.png"						
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"dest": "_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"default",
			"templates/material"
		],
		"xrefService": [
			"https://xref.docs.microsoft.com/query?uid={uid}"
		],
		"postProcessors": [],
		"markdownEngineName": "markdig",
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false,
		"disableGitFeatures": false,
		"globalMetadata": {
			"_appTitle": "PnP Syntex Samples",
			"_appFooter": "<span>Syntex Samples<br>Generated by <strong><a href='https://dotnet.github.io/docfx' alt='Doc FX Website'>DocFX</a></strong> with <strong><a href='https://ovasquez.github.io/docfx-material' alt='DocFx Material Theme'>Material UI</a></strong></span>",
			"_appFaviconPath": "https://c.s-microsoft.com/favicon.ico?v2",
			"_appLogoPath": "",
			"_description": "This repository contains community samples that demonstrate different usage patterns and templates for Microsoft SharePoint Syntex.",
			"_enableSearch": true,
			"_enableNewTab": true,
			"_disableBreadcrumb": true
		}
	}
} 